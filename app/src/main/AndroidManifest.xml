<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adhito.inixindo_task_individual">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Inixindo_Task_Individual"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MateriTambahActivity"
            android:exported="false" />
        <activity
            android:name=".MateriDetailActivity"
            android:exported="false" />
        <activity
            android:name=".InstrukturTambah"
            android:exported="false" />
        <activity
            android:name=".InstrukturDetail"
            android:exported="false" />
        <activity
            android:name=".PesertaDetailActivity"
            android:exported="false" />
        <activity
            android:name=".PesertaTambahActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>